{
  "compilerOptions": {
    "target": "ES2020",
    "jsx": "react-jsx",
    "module": "ES6",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["./src/*"]
    },
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "alwaysStrict": true,
    "skipLibCheck": true
  },
  "include": ["src", "./react-app-env.d.ts"],
  "exclude": ["node_modules"]
}
